{
    "Function Declaration": {
        "prefix": "fn",
        "body": [
            "fn ${1:name}(${2:params}) {",
            "    $0",
            "}"
        ],
        "description": "Cria uma nova função"
    },
    "Variable Declaration": {
        "prefix": "let",
        "body": [
            "let ${1:name} = ${2:value};"
        ],
        "description": "Declara uma variável"
    },
    "If Statement": {
        "prefix": "if",
        "body": [
            "if (${1:condition}) {",
            "    $0",
            "}"
        ],
        "description": "Estrutura condicional If"
    },
    "While Loop": {
        "prefix": "while",
        "body": [
            "while (${1:condition}) {",
            "    $0",
            "}"
        ],
        "description": "Laço de repetição While"
    },
    "Print Statement": {
        "prefix": "print",
        "body": [
            "print(${1:expression});"
        ],
        "description": "Imprime no console"
    }
}
